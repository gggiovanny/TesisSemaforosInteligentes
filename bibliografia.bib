  issn     = {09252312},
  journal  = {Neurocomputing},
  keywords = {Genetic Algorithm,Optimization,Petri net,Urban traffic},
  pages    = {162--167},
  title    = {{Optimizing urban traffic flow using Genetic Algorithm with Petri net analysis as fitness function}},
  volume   = {124},
  year     = {2014}
}
